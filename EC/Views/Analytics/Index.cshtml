@using EC.Models;
@using EC.Models.ECModel;
@using EC.Models.Database;
@using System.Data;
@using EC.Localization;
@{

    <link rel="stylesheet" href="~/Content/styleAnalytics.css" />
  bool? is_cc = ViewBag.is_cc;
  if (!is_cc.HasValue)
  {
    is_cc = false;
  }
  ViewBag.Title = LocalizationGetter.GetString("Analytics", is_cc);
  Layout = "~/Views/Shared/IndexPages/LayoutCasesNewReport.cshtml";
  int user_id = (Int32)ViewBag.user_id;
  ReportModel rm = new ReportModel();
  int companyId = ViewBag.um._user.company_id;
  int[] _today_spanshot = ViewBag._today_spanshot;
  int[] _month_end_spanshot = ViewBag._month_end_spanshot;
  int difference = 0;

  string _today = ViewBag._today;

  string[] _titleHeaderLegend = { "Spam", "New Report", "Report Review", "Under Investigation", "Awaiting Sign-Off", "Closed" };
  int[] _titleHeaderLegendIdx = { 6, 0, 1, 2, 3, 8 };

  string[] _miniSquareColor = { "#abb9bb", "#d47472", "#ff9b42", "#3099be", "#64cd9b", "#e5eeee" };

}

<!-- Include Date Range Picker -->
<script src="~/Scripts/moment.min.js"></script>
<script src="~/Scripts/jquery.daterangepicker.min.js"></script>
<link rel="stylesheet" href="~/Libs/nvd3/build/nv.d3.min.css">

<link href="~/Content/daterangepickerAnalytic.css" rel="stylesheet" />

<input type="hidden" id="companyId" value="@companyId" />
<input type="hidden" id="user_id" value="@user_id" />
<input type="hidden" id="dateStart" value="" />
<input type="hidden" id="dateEnd" value="" />

<input type="hidden" id="step1_delay" value="@ViewBag.step1_delay" />
<input type="hidden" id="step2_delay" value="@ViewBag.step2_delay" />
<input type="hidden" id="step3_delay" value="@ViewBag.step3_delay" />
<input type="hidden" id="step4_delay" value="@ViewBag.step4_delay" />

<style>
    #menu a {
        max-width: 500px;
    }
</style>

<div class="blockSettings blockAnalitics">
    @Html.Partial("~/Views/Analytics/AnalyticsMenu.cshtml")

    <!--work-->
    <div class="dropDownMenuRow">
        <div class="container">
            <div class="parentClass">
                <div class="menuItem">
                    <div class="positionText">
                        <span class="dropMenuText">@LocalizationGetter.GetString("Department", is_cc)</span>
                        <img src="~/Content/img/arrowSelect.png" />
                    </div>
                    <ul class="menuUl">
                        @if (ViewBag.dropDownFirst != null && ViewBag.dropDownFirst.Count > 0)
                        {
                            foreach (Tuple<string, string> tuple in ViewBag.dropDownFirst)
                            {
                                <li>
                                    <div class="liItem" value="@tuple.Item2" nameDropdown="ReportsDepartmentIDStringss">
                                        <div class="checked">
                                            <img class="logo" src="~/Content/img/check.png" />
                                        </div>
                                        <span class="positionText liText">
                                            @tuple.Item1
                                        </span>
                                    </div>
                                </li>
                            }
                        }
                    </ul>
                </div>
            </div>
            <div class="parentClass">
                <div class="menuItem">
                    <div class="positionText">
                        <span class="dropMenuText">@LocalizationGetter.GetString("Location", is_cc)</span>
                        <img src="~/Content/img/arrowSelect.png" />
                    </div>
                    <ul class="menuUl">
                        @if (ViewBag.dropDownSecond != null && ViewBag.dropDownSecond.Count > 0)
                        {
                            foreach (Tuple<string, string> tuple in ViewBag.dropDownSecond)
                            {
                                <li>
                                    <div class="liItem" value="@tuple.Item2" nameDropdown="ReportsLocationIDStrings">
                                        <div class="checked">
                                            <img class="logo" src="~/Content/img/check.png" />
                                        </div>
                                        <span class="positionText liText">
                                            @tuple.Item1
                                        </span>
                                    </div>
                                </li>
                            }
                        }
                    </ul>
                </div>
            </div>
            <div class="parentClass">
                <div class="menuItem">
                    <div class="positionText">
                        <span class="dropMenuText">@LocalizationGetter.GetString("IncidentType", is_cc)</span>
                        <img src="~/Content/img/arrowSelect.png" />
                    </div>
                    <ul class="menuUl">
                        @if (ViewBag.dropDownThird != null && ViewBag.dropDownThird.Count > 0)
                        {
                            foreach (Tuple<string, string> tuple in ViewBag.dropDownThird)
                            {
                                <li>
                                    <div class="liItem" value="@tuple.Item2" nameDropdown="ReportsSecondaryTypesIDStrings">
                                        <div class="checked">
                                            <img class="logo" src="~/Content/img/check.png" />
                                        </div>
                                        <span class="positionText liText">
                                            @tuple.Item1
                                        </span>
                                    </div>
                                </li>
                            }
                        }
                    </ul>
                </div>
            </div>
            <div class="parentClass">
                <div class="menuItem">
                    <div class="positionText">
                        <span class="dropMenuText">Reporter Type</span>
                        <img src="~/Content/img/arrowSelect.png" />
                    </div>
                    <ul class="menuUl">
                        @if (ViewBag.dropDownFourth != null && ViewBag.dropDownFourth.Count > 0)
                        {
                            foreach (Tuple<string, string> tuple in ViewBag.dropDownFourth)
                            {
                                <li>
                                    <div class="liItem" value="@tuple.Item2" nameDropdown="ReportsRelationTypesIDStrings">
                                        <div class="checked">
                                            <img class="logo" src="~/Content/img/check.png" />
                                        </div>
                                        <span class="positionText liText">
                                            @tuple.Item1
                                        </span>
                                    </div>
                                </li>
                            }
                        }
                    </ul>
                </div>
            </div>
            <div class="daterange menuItem">
                <div class="positionText">
                    <span class="dropMenuText">Date range</span>
                    <img src="~/Content/img/arrowSelect.png" />
                </div>
                <input id="daterange" type="text" style="display: none" name="daterange" value="01/01/2015 - 01/31/2015" />
            </div>
        </div>
    </div>


    <!--end work-->
    <div class="borderAnalit">

        <div class="container">
            <div class="filter">
                <table>
                    <tbody>
                        <tr>
                            <td class="filterUp">Search:</td>
                            <td>
                                <table id="filterTable"></table>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div><!-- /.container -->

        <div class="container">
            <div class="contentSettings contentActivity">
                <!-- 1 All cases -->
                <div class="blockAllCases">
                    <div class="headerBlockActivity">
                        <div class="titleBlock">@LocalizationGetter.GetString("Allcases", is_cc)</div>
                        @for (var i = 0; i < 5; i++)
                    {
                    <div class="itemLegend">
                        <div class="miniSquare" style="background-color: @_miniSquareColor[i];"></div>
                        <span>@_titleHeaderLegend[i]</span>
                    </div>
                    }
                    </div>

                    <div id="chartContainer"></div>
                </div>
                <!-- 2 Today -->
                <div class="blockToday">
                    <div class="headerBlockActivity">
                        <div class="titleBlock">@LocalizationGetter.GetString("Number_cases_stage", is_cc): @_today</div>
                        <div class="blockTodayContant">
                            @for (var i = 0; i < _titleHeaderLegend.Length; i++)
                            {
                                <div class="itemLegend">
                                    <div class="headerLegend">
                                        <div class="miniSquare" style="background-color: @_miniSquareColor[i];"></div>
                                        <span>@_titleHeaderLegend[i]</span>
                                    </div>
                                    <div class="contentLegend">
                                        <label>@_today_spanshot[_titleHeaderLegendIdx[i]]</label>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>

                </div>
                <!-- 3 Average -->


                <!-- 4 Departments & Locations -->
                <div class="bigBlock" style="width: 100%;">
                    <!-- 4_1 Departments -->
                    <div class="blockDepartments" style="width: 800px; margin: 0 auto; float: none;">
                        <div style="width: 100%; display: table;">
                            <div id="containerDepartments" class="containers" style="width: 95%; margin-left: 5%;"></div>
                        </div>
                    </div>
                </div>
                <div class="bigBlock" style="width: 100%;">
                    <!-- 4_2 Locations -->
                    <div class="blockLocationsAN" style="width: 800px; margin: 0 auto; float: none;">
                        <div style="width: 100%; display: table;">
                            <div id="containerLocation" class="containers" style="width: 95%; margin-left: 5%;"></div>
                        </div>

                    </div>
                </div>

                <div class="bigBlock" style="width: 100%;">
                    <div class="blockTypeOfIncident" style="width: 800px; margin: 0 auto; float: none;">
                        <div id="containerTypesOfIncident" class="containers" style="width: 95%; margin-left: 5%;"></div>
                    </div>
                </div>
                <div class="bigBlock" style="width: 100%;">
                    <div class="blockReporter" style="width: 800px; margin: 0 auto; float: none;">
                        <div style="width: 100%; display: table;">
                            <div id="containerTypeOfReporter" class="containers" style="width: 95%; margin-left: 5%;"></div>
                        </div>
                    </div>
                </div>
            </div><!--contentSettings-->
        </div>
    
    </div>
</div><!--end blockSettings blockAnalitics-->
<meta http-equiv="content-type" content="text/html; charset=UTF-8">
<script type='text/javascript' src="https://ajax.aspnetcdn.com/ajax/globalize/0.1.1/globalize.min.js"></script>
@*<script type='text/javascript' src="https://cdn3.devexpress.com/jslib/13.1.5/js/dx.chartjs.js"></script>*@
<script type='text/javascript' src="https://cdn3.devexpress.com/jslib/18.1.3/js/dx.all.js"></script>

<script>
    $(document).ready(function () {

        //START Open mini menu for mobile
        function blockActivityHeight() {
            if ($('#menu').height() < 50) {
                $('.positionActivityIcon').height(89);
            }
            else {
                $('.positionActivityIcon').height($('#casesHeared').height());
            }
        }
        $('.mainTitle').click(function () {
            $('.mainTitle + div').toggle();
            blockActivityHeight();
        });
    });
</script>
<script src="~/Scripts/indexAnalytics.js"></script>