                '/Libs/ng-file-upload/ng-file-upload.min.js',
                '/Libs/angular-ui-calendar/src/calendar.js'
            ];
            array.forEach(function (element) {
                var js = document.createElement("script");
                js.type = "text/javascript";
                js.src = element;
                head.appendChild(js);
            });
        }

    </script>

